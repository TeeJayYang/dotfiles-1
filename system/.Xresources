!! Xft settings
Xft.dpi:                  96
Xft.antialias:            true
Xft.rgba:                 rgb
Xft.hinting:              true
Xft.hintstyle:            hintslight

!! Colors
*background:              #002b36
*foreground:              #829496
*fadeColor:               #002b36
*cursorColor:             #93a1a1
*pointerColorBackground:  #586e75
*pointerColorForeground:  #93a1a1

!! Scrollbar
URxvt.scrollstyle:        plain
URxvt.scrollColor:        #777777
URxvt.scrollBar_right:    true
URxvt.thickness:          2
URxvt.internalBorder:     5

!! Font
URxvt.font:               -misc-tamzenforpowerline-medium-r-normal--16-116-100-100-c-80-iso8859-1
URxvt.boldFont:           -misc-tamzenforpowerline-bold-r-normal--16-116-100-100-c-80-iso8859-1
URxvt.letterSpace:        0.0

!! Real transparency
URxvt.depth:              32
URxvt.background:         [90]#001115

!! Fake transparency
!urxvt.transparent:       true
!urxvt.shading:           108
!urxvt.tintColor:         #002b36

!! Blur
!urxvt.blurRadius:        10

!! Colors
! Base 03
*color8:                  #002b36
! Base 02
*color0:                  #073642
! Base 01
*color10:                 #586e75
! Base 00
*color11:                 #657b83
! Base 0
*color12:                 #839496
! Base 1
*color14:                 #93a1a1
! Base 2
*color7:                  #eee8d5
! Base 3
*color15:                 #fdf6e3
! Yellow
*color3:                  #b58900
! Orange
*color9:                  #cb4b16
! Red
*color1:                  #dc322f
! Magenta
*color5:                  #d33682
! Violet
*color13:                 #6c71c4
! Blue
*color4:                  #268bd2
! Cyan
*color6:                  #2aa198
! Green
*color2:                  #859900

!! Extensions
URxvt.perl-ext-common:    default,matcher
URxvt.url-launcher:       /usr/bin/xdg-open
URxvt.matcher.button:     1

!! Other Settings
URxvt.termName:           xterm-256color
URxvt.saveLines:          1000000
URxvt.buffered:           true

!! Rofi config
rofi.lines:               10
rofi.eh:                  2
rofi.width:               100
rofi.padding:             300
rofi.bw:                  0
rofi.yoffset:             0
rofi.font:                TamzenForPowerline 16
rofi.fullscreen:          true

rofi.color-enabled:       true
rofi.color-window:        argb:f2002b36, argb:f2002b36, argb:f2002b36
rofi.color-normal:        argb:00002b36, argb:f2b58900, argb:00002b36, argb:f2b58900, argb:f2002b36
rofi.color-urgent:        argb:00002b36, argb:f2b58900, argb:00002b36, argb:f2b58900, argb:f2002b36
rofi.color-active:        argb:00002b36, argb:f2b58900, argb:00002b36, argb:f2b58900, argb:f2002b36

rofi.terminal:            urxvt
rofi.ssh-client:          ssh
rofi.ssh-command:         {terminal} -e {ssh-client} {host}
